@{
    ViewData["Title"] = "Home Page";
}

<div class="container mt-5">
	<div class="row justify-content-center">
		<div class="col-md-4 home-Images">
			<img src="Search.png" alt="Search image" />
			<a class="btn btn-primary" asp-controller="Hero" asp-action="Search">Search hero</a>
		</div>
		<div class="col-md-4 home-Images">
			<img src="compare.png" alt="vs Image" />
			<a class="btn btn-primary" asp-controller="Compare" asp-action="Index">Compare</a>
		</div>
		<div class="col-md-4 home-Images">
			<img src="Spider.png" alt="Spiderman img" />
			<a class="btn btn-primary" asp-controller="Alignment" asp-action="">Allignment</a>
		</div>
	</div>
</div>


